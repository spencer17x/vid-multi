{
  "name": "vid-multi",
  "version": "0.0.11",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "tsc && vite build --mode prod  && electron-builder",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "ci-publish": "ar-changelog publish",
    "release": "ar-changelog release",
    "clean": "rimraf dist dist-electron release",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:mwl": "npm run build && electron-builder -mwl"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.10.1",
    "dayjs": "^1.11.10",
    "electron-updater": "^6.1.4",
    "mediainfo.js": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "ar-changelog": "4.1.0-beta.0",
    "conventional-changelog-cli": "^2.2.2",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-static-copy": "^0.17.0"
  },
  "main": "dist-electron/main.js"
}
